{
    "version": 2,
    "outputDirectory": "dist/public",
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 0 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}